{
  "name": "go-size-analyzer-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build:ui": "tsc && vite -c vite.config.ts build",
    "build:explorer": "tsc && vite -c vite.config-explorer.ts build",
    "dev:ui": "vite -c vite.config.ts",
    "dev:explorer": "vite -c vite.config-explorer.ts",
    "generate": "typia generate --input src/schema --output src/generated --project tsconfig.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.20",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@codecov/vite-plugin": "0.0.1-beta.10",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@types/d3-array": "^3.2.1",
    "@types/d3-color": "^3.1.3",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-scale": "^4.0.8",
    "@types/golang-wasm-exec": "^1.15.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.0",
    "junit": "^1.4.9",
    "sass": "^1.77.5",
    "terser": "^5.31.1",
    "typescript": "^5.5.2",
    "typia": "^6.3.1",
    "vite": "^5.3.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^2.0.1",
    "vitest": "^1.6.0"
  }
}
