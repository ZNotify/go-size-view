    "d3-color": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "lodash.memoize": "^4.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@codecov/vite-plugin": "0.0.1-beta.10",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@million/lint": "1.0.0-rc.84",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
