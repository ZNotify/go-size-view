{
  "name": "go-size-analyzer-ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:ui": "tsc && vite -c vite.config.ts build",
    "build:explorer": "tsc && vite -c vite.config-explorer.ts build",
    "dev:ui": "vite -c vite.config.ts",
    "dev:explorer": "vite -c vite.config-explorer.ts",
    "generate": "typia generate --input src/schema --output src/generated --project tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "@types/lodash.memoize": "^4.1.9",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "lodash.memoize": "^4.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@codecov/vite-plugin": "1.0.1",
    "@eslint-react/eslint-plugin": "^1.14.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3-array": "^3.2.1",
    "@types/d3-color": "^3.1.3",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-scale": "^4.0.8",
    "@types/golang-wasm-exec": "^1.15.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/web-worker": "^2.1.1",
    "eslint": "^9.10.0",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "sass": "^1.78.0",
    "terser": "^5.32.0",
    "typescript": "^5.6.2",
    "typia": "^6.10.0",
    "vite": "^5.4.5",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^2.0.2",
    "vitest": "^2.1.1",
    "vitest-fetch-mock": "^0.3.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    },
    "overrides": {
      "micromatch": "4.0.8"
    }
  }
}
