{
  "name": "go-size-analyzer-ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:ui": "tsc && vite -c vite.config.ts build",
    "build:explorer": "tsc && vite -c vite.config-explorer.ts build",
    "dev:ui": "vite -c vite.config.ts",
    "dev:explorer": "vite -c vite.config-explorer.ts",
    "generate": "typia generate --input src/schema --output src/generated --project tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@types/lodash.memoize": "^4.1.9",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "lodash.memoize": "^4.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@codecov/vite-plugin": "1.6.0",
    "@eslint-react/eslint-plugin": "^1.22.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3-array": "^3.2.1",
    "@types/d3-color": "^3.1.3",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-scale": "^4.0.8",
    "@types/golang-wasm-exec": "^1.15.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jsdom": "^25.0.1",
    "sass": "^1.83.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "typia": "^7.5.1",
    "vite": "^6.0.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-singlefile": "^2.1.0",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    },
    "overrides": {
      "micromatch": "4.0.8"
    }
  }
}
